{
	"name": "Dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "TwitterJson1",
						"type": "DatasetReference"
					},
					"name": "TwitterJSON"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "TwitterJson1",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          results as (label as string, name as string, volume as integer, retweets as integer, tweets as integer, reachEstimate as integer, impressions as integer)[],",
				"          orderBy as string,",
				"          orderDirection as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     documentForm: 'documentPerLine') ~> TwitterJSON",
				"TwitterJSON sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          results as (label as string, name as string, volume as integer, retweets as integer, tweets as integer, reachEstimate as integer, impressions as integer)[],",
				"          orderBy as string,",
				"          orderDirection as string",
				"     ),",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sink1"
			]
		}
	}
}