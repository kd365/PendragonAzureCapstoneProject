{
	"name": "BrandwatchHttpServer",
	"properties": {
		"annotations": [],
		"type": "HttpServer",
		"typeProperties": {
			"url": "https://api.brandwatch.com/",
			"enableServerCertificateValidation": true,
			"authenticationType": "Anonymous",
			"authHeaders": {
				"Authorization": {
					"type": "AzureKeyVaultSecret",
					"store": {
						"referenceName": "AzureKeyVaultRedditAppTest",
						"type": "LinkedServiceReference"
					},
					"secretName": "Brandwatch-AccessToken"
				}
			}
		}
	}
}