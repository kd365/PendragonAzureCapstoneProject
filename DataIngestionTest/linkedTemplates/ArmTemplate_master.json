{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "DataIngestionTest"
		},
		"AzureBlobStorageRedditAppTest_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorageRedditAppTest'"
		},
		"AzureDataLakeStorageRedditAppTest_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorageRedditAppTest'"
		},
		"RedditAppTestDataLakeStorage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'RedditAppTestDataLakeStorage'"
		},
		"AzureDataLakeStorageRedditAppTest_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://pendragonteststorage.dfs.core.windows.net/"
		},
		"AzureKeyVaultRedditAppTest_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://tw-dv-001.vault.azure.net/"
		},
		"HttpRedditServer_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://www.reddit.com/r/dataengineering/"
		},
		"HttpRedditServer_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "ryan_supplyin"
		},
		"RedditAppTestDataLakeStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://cs2100320012bda2056.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "DataIngestionTest_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorageRedditAppTest_connectionString": {
						"value": "[parameters('AzureBlobStorageRedditAppTest_connectionString')]"
					},
					"AzureDataLakeStorageRedditAppTest_accountKey": {
						"value": "[parameters('AzureDataLakeStorageRedditAppTest_accountKey')]"
					},
					"RedditAppTestDataLakeStorage_accountKey": {
						"value": "[parameters('RedditAppTestDataLakeStorage_accountKey')]"
					},
					"AzureDataLakeStorageRedditAppTest_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorageRedditAppTest_properties_typeProperties_url')]"
					},
					"AzureKeyVaultRedditAppTest_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVaultRedditAppTest_properties_typeProperties_baseUrl')]"
					},
					"RedditAppTestDataLakeStorage_properties_typeProperties_url": {
						"value": "[parameters('RedditAppTestDataLakeStorage_properties_typeProperties_url')]"
					},
					"HttpRedditServer_properties_typeProperties_url": {
						"value": "[parameters('HttpRedditServer_properties_typeProperties_url')]"
					},
					"HttpRedditServer_properties_typeProperties_userName": {
						"value": "[parameters('HttpRedditServer_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}