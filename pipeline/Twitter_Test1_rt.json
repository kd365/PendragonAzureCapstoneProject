{
	"name": "Twitter_Test1_rt",
	"properties": {
		"activities": [
			{
				"name": "Twitter NATO mentions",
				"description": "2023-02-19T00:00:07.000Z to 2023-02-20T00:00:07.000Z\nmax_results=10",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "GET",
						"paginationRules": {
							"supportRFC5988": "true"
						}
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings",
							"copyBehavior": "PreserveHierarchy"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".csv"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['data'][0]['edit_history_tweet_ids'][0]"
								},
								"sink": {
									"ordinal": 1
								}
							},
							{
								"source": {
									"path": "$['data'][0]['id']"
								},
								"sink": {
									"ordinal": 2
								}
							},
							{
								"source": {
									"path": "$['data'][0]['text']"
								},
								"sink": {
									"ordinal": 3
								}
							},
							{
								"source": {
									"path": "$['meta']['newest_id']"
								},
								"sink": {
									"ordinal": 4
								}
							},
							{
								"source": {
									"path": "$['meta']['oldest_id']"
								},
								"sink": {
									"ordinal": 5
								}
							},
							{
								"source": {
									"path": "$['meta']['result_count']"
								},
								"sink": {
									"ordinal": 6
								}
							},
							{
								"source": {
									"path": "$['meta']['next_token']"
								},
								"sink": {
									"ordinal": 7
								}
							}
						],
						"mapComplexValuesToString": false
					}
				},
				"inputs": [
					{
						"referenceName": "RestResourceTwitter1",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "Twitter_DelimitedText1",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}