				"source": [
					"from pyspark.sql.functions import regexp_replace\n",
					"\n",
					"# Define the regular expression pattern to match words starting with @, &, or https\n",
					"#regex = r'@\\w+|&\\w+|https\\w+|#\\w+'\n",
					"regex = r'\\b[@#&]\\w+|\\bhttps\\w*'\n",
					"regex = r'(?:#|@)\\w+|http\\S+'\n",
					"\n",
					"# Apply the regexp_replace function to the text column and create a new column with the filtered text\n",
					"df_filtered = df.withColumn(\"text\", regexp_replace(col(\"text\"), regex, ''))\n",
					"\n",
					"# Show the filtered DataFrame\n",
					"df_filtered.select(\"text\").show()\n",
					"\n",
					"cleaned_df = df_filtered"
				],
				"execution_count": 11
