				"source": [
					"import re\n",
					"emoticon_pattern = r'[\\uD83C-\\uDBFF\\uDC00-\\uDFFF]+'\n",
					"# apply the regex pattern to the 'text' column to remove emoticons\n",
					"cleaned_df = cleaned_df.withColumn(\"text\", regexp_replace(col(\"text\"), emoticon_pattern, ''))\n",
					"\n",
